  },
  "author": "SLIGHTNING",
  "scripts": {
    "auto-reverse": "npm run build:scripts && npm run auto-reverse-only && npm run lint:fix-only",
    "auto-reverse-only": "npm run unpack:all-only && npm run transform-only",
    "unpack:all": "npm run build:scripts && npm run unpack:all-only",
    "unpack:all-only": "npm run unpack:home-only && npm run unpack:shared-only && npm run unpack:editor-only && npm run unpack:editor-service-worker-only && npm run unpack:editor-player-only && npm run unpack:player-only && npm run unpack:about-only",
    "unpack:home": "npm run build:scripts && npm run unpack:home-only",
    "unpack:home-only": "node ./out/scripts/unpack/home.js",
    "unpack:shared": "npm run build:scripts && npm run unpack:shared-only",
    "unpack:shared-only": "node ./out/scripts/unpack/shared.js",
    "unpack:editor": "npm run build:scripts && npm run unpack:editor-only",
    "unpack:editor-only": "node ./out/scripts/unpack/editor.js",
    "unpack:editor-service-worker": "npm run build:scripts && npm run unpack:editor-service-worker-only",
    "unpack:editor-service-worker-only": "node ./out/scripts/unpack/editor-service-worker.js",
    "unpack:editor-player": "npm run build:scripts && npm run unpack:editor-player-only",
    "unpack:editor-player-only": "node ./out/scripts/unpack/editor-player.js",
    "unpack:player": "npm run build:scripts && npm run unpack:player-only",
    "unpack:player-only": "node ./out/scripts/unpack/player.js",
    "unpack:about": "npm run build:scripts && npm run unpack:about-only",
    "unpack:about-only": "node ./out/scripts/unpack/about.js",
    "transform": "npm run build:scripts && npm run transform-only",
    "transform-only": "node ./out/scripts/transform/babel/index.js",
    "lint": "npm run build:scripts && npm run lint-only",
    "lint-only": "eslint src unrestored src-unrestored",
    "lint:fix": "npm run build:scripts && npm run lint:fix-only",
    "lint:fix-only": "eslint --fix src unrestored src-unrestored",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "start": "rspack serve --config rspack.dev.js",
    "build": "rspack --config rspack.prod.js",
    "build:dev": "rspack --config rspack.dev.js",
    "serve": "rspack serve --config rspack.server.js"
  },
